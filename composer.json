{
    "name": "simpspector/analyser",
    "type": "lib",
    "description": "SimpSpector Analyser",
    "autoload": {
        "psr-4": {
            "SimpSpector\\Analyser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpSpector\\Analyser\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "~5.4 || ~7.0",
        "symfony/console": "^2.7 || ^3.0",
        "symfony/event-dispatcher": "^2.7 || ^3.0",
        "symfony/yaml": "^2.7 || ^3.0",
        "symfony/options-resolver": "^2.7 || ^3.0",
        "symfony/finder": "^2.7 || ^3.0",
        "symfony/process": "^2.7 || ^3.0",
        "symfony/serializer": "^2.7 || ^3.0",
        "symfony/dependency-injection": "^2.7 || ^3.0",
        "symfony/config": "^2.7 || ^3.0",
        "pdepend/pdepend": "~2.2",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~2.0",
        "sensiolabs/security-checker": "~2.0 || ~3.0",
        "nikic/php-parser": "^2.0",
        "simpspector/twig-lint": "dev-symfony-3.0",
        "davidbadura/markdown-builder": "~1.0.3",
        "jms/serializer": "~1.0",
        "webmozart/path-util": "^2.0",
        "phpunit/php-token-stream": "1.*",
        "phpdocumentor/reflection-docblock": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8.20 || ~5.0",
        "symfony/var-dumper": "^2.7 || ^3.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": [
        "bin/simpspector"
    ]
}
