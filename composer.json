{
    "name": "simpspector/analyser",
    "type": "lib",
    "description": "SimpSpector Analyser",
    "autoload": {
        "psr-4": {"SimpSpector\\Analyser\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"SimpSpector\\Analyser\\Tests\\": "tests/"}
    },
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.6",
        "symfony/event-dispatcher": "~2.6",
        "symfony/yaml": "~2.6",
        "symfony/options-resolver": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/process": "~2.6",
        "symfony/serializer": "~2.6",
        "symfony/dependency-injection": "~2.6",
        "symfony/config": "~2.6",
        "pdepend/pdepend": "~2.0.4",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~2.0",
        "sensiolabs/security-checker": "~2.0",
        "nikic/php-parser": "~1.2.0",
        "simpspector/twig-lint": "~1.1",
        "davidbadura/markdown-builder": "~1.0.3",
        "jms/serializer": "0.16.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "bin": ["bin/simpspector"]
}
