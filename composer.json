{
    "name": "simpspector/analyser",
    "type": "lib",
    "description": "SimpSpector Analyser",
    "autoload": {
        "psr-4": {"SimpSpector\\Analyser\\": "src/"}
    },
    "dev-autoload": {
        "psr-4": {"SimpSpector\\Analyser\\Tests\\": "tests/"}
    },
    "require": {
        "php": ">=5.4",
        "symfony/console": "~2.6",
        "symfony/yaml": "~2.6",
        "symfony/options-resolver": "~2.6",
        "symfony/finder": "~2.6",
        "symfony/process": "~2.6",
        "symfony/serializer": "~2.6",
        "phpmd/phpmd": "~2.1",
        "squizlabs/php_codesniffer": "~2.0",
        "nikic/php-parser": "~1.0.2",
        "asm89/twig-lint": "dev-fix/twig-gt-1.16.2",
        "davidbadura/markdown-builder": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "repositories": [
        { "type": "vcs", "url": "https://github.com/tolry/twig-lint.git" }
    ]
}
